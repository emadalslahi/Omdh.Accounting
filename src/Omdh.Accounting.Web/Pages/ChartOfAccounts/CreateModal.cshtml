@page
@using Omdh.Accounting.Localization
@using Omdh.Accounting.Web.Pages.ChartOfAccounts
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<AccountingResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="ChartOfAccountDto" asp-page="/ChartOfAccounts/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["New Account"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>